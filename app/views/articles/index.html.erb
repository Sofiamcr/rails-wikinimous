
<div class= "container d-flex justify-content-center">
  <div class= "row" >
      <article>
          <% @articles.each do |article| %>
            <h2><%= link_to article.title, article_path(article) %></h2>
            <p><%= article.content %></p>
          <% end %>
      </article>
  </div>
</div>
